// TOKEN types
// TOKEN_TYPE(name, val, specificator, ... - arguments)
TOKEN_TYPE(STATEMENT, , %c)
TOKEN_TYPE(INSTRUCTION, , %s)
TOKEN_TYPE(INITIALIZATOR, , )
TOKEN_TYPE(FUNCTION_RET_TYPE, ,)
TOKEN_TYPE(OPERATOR, , %c)
// string
TOKEN_TYPE(NAME, , %s)
TOKEN_TYPE(VARIABLE, ,)
TOKEN_TYPE(CONSTANT, ,%lg)
TOKEN_TYPE(FUNCTION, ,)
TOKEN_TYPE(CALL, , )
TOKEN_TYPE(ASSIGMENT, '=', )
TOKEN_TYPE(BLOCK_OPENING_BRACKET, '{')
TOKEN_TYPE(BLOCK_CLOSING_BRACKET, '}')
TOKEN_TYPE(EXPRESSION_OPENING_BRACKET, '(')
TOKEN_TYPE(EXPRESSION_CLOSING_BRACKET, ')')
TOKEN_TYPE(END_OF_STATEMENT, ';')